<?xml version="1.0" encoding="UTF-8"?>
<ivy-module version="2.0" xmlns:m="http://ant.apache.org/ivy/maven">
  <info organisation="${ivy.artifact.group}" module="${ivy.artifact.id}" revision="${project.revision}" />

  <configurations>
    <conf name="default" />
    <conf name="test" visibility="private" />
  </configurations>

  <publications>
    <artifact name="${ivy.artifact.id}" type="jar" conf="default" />
  </publications>

  <dependencies defaultconf="default->default">
    <dependency org="org.apache.commons" name="commons-vfs2" rev="2.1-20150824"/>
    <dependency org="pentaho-kettle" name="kettle-core" rev="${dependency.kettle.revision}" changing="true" />
    <dependency org="log4j" name="log4j" rev="1.2.14"/>
    <dependency org="pentaho-kettle" name="kettle-engine" rev="${dependency.kettle.revision}" changing="true" />
    <dependency org="org.osgi" name="org.osgi.core" rev="4.3.1" changing="true" />

    <dependency conf="test->default" org="junit" name="junit" rev="4.11"/>
    <dependency conf="test->default" org="commons-logging" name="commons-logging" rev="1.1.1"/>
    <dependency conf="test->default" org="org.jboss.shrinkwrap" name="shrinkwrap-impl-base" rev="1.0.0-alpha-12" />
    <dependency conf="test->default" org="org.mockito" name="mockito-all" rev="1.9.5" transitive="false" />
  </dependencies>
</ivy-module>
